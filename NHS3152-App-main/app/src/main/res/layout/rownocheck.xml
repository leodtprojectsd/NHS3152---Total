<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"


    xmlns:android="http://schemas.android.com/apk/res/android">



    <TableRow android:padding="5dp"
        android:layout_marginBottom="5dp"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:weightSum="13" >
        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="4"
            android:id="@+id/dataTable"
            android:textSize="10dp"
            android:gravity="center"
            android:fontFamily="@font/glegoo"/>



        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:id="@+id/codTable"
            android:gravity="center"
            android:textSize="10dp"
            android:fontFamily="@font/glegoo"/>
        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:id="@+id/rTable"
            android:gravity="center"
            android:textSize="10dp"
            android:fontFamily="@font/glegoo"/>
        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:id="@+id/v1Table"

            android:textSize="10dp"
            android:fontFamily="@font/glegoo"/>
        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:id="@+id/v2Table"

            android:textSize="10dp"
            android:fontFamily="@font/glegoo"/>
        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:id="@+id/vdsTable"

            android:textSize="10dp"
            android:fontFamily="@font/glegoo"/>
        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:id="@+id/ipaTable"

            android:textSize="10dp"
            android:fontFamily="@font/glegoo"/>
        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:id="@+id/nativeTable"
            android:gravity="center"
            android:textSize="10dp"
            android:fontFamily="@font/glegoo"/>

    </TableRow>

</TableLayout>